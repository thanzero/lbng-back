{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "MongoDB",
    "public": false,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "AccessToken": {
    "dataSource": "MongoDB",
    "public": false
  },
  "ACL": {
    "dataSource": "MongoDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "MongoDB",
    "public": true,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "MongoDB",
    "public": true
  },
  "Email": {
    "dataSource": "mailer",
    "public": false
  },
  "Account": {
    "dataSource": "MongoDB",
    "public": true
  },
  "Article": {
    "dataSource": "MongoDB",
    "public": true
  },
  "container": {
    "dataSource": "files",
    "public": true
  },
  "Tag": {
    "dataSource": "MongoDB",
    "public": true
  },
  "TagLink": {
    "dataSource": "MongoDB",
    "public": false
  },
  "Page": {
    "dataSource": "MongoDB",
    "public": true
  },
  "MenuItem": {
    "dataSource": "MongoDB",
    "public": true
  }
}
